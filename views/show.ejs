<% include ./partials/header.ejs %>

<div class="w-full min-h-screen bg-zinc-700 px-10 py-5 text-white " >

    <div  class="cards flex flex-wrap gap-10 ">
        <% user.posts.forEach(ele => {  %>

            <div class=" card w-32">
                <div class=" w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
                      <img src="/images/uploads/<%= ele.image %>" alt="" class="w-full h-full object-cover">
                </div>
                <h5 class="mt-2 text-lg"> <%= ele.title %> </h5>
                <h5 class=" text-sm"> <%= ele.description %> </h5>
            </div>


            
       <%   }); %>

       
    </div>


 

</div>

<% include ./partials/footer.ejs %>